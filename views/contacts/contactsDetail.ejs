<% include ../header.ejs %>

    <div class="panel panel-default">
        <div class="panel-heading">
            <%=contact.name %>
        </div>
        <div class="panel-body">
            <div style="padding-bottom: 10px">
                작성일 : <%=contact.getDate.year%> - <%=contact.getDate.month%> - <%=contact.getDate.day%>
